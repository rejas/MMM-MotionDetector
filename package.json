{
  "name": "mmm-motiondetector",
  "version": "1.6.0",
  "description": "a motion detector module for MagicMirror",
  "author": "rejas@github.com",
  "license": "MIT",
  "bugs": "https://github.com/rejas/MMM-MotionDetector/issues",
  "homepage": "https://github.com/rejas/MMM-MotionDetector#readme",
  "repository": "github:rejas/MMM-MotionDetector.git",
  "keywords": [
    "magic",
    "mirror",
    "smart",
    "mirror",
    "motion detection"
  ],
  "scripts": {
    "lint": "eslint && prettier . --check",
    "lint:fix": "eslint --fix && prettier . --write",
    "prepare": "[ -f node_modules/.bin/husky ] && husky || echo info for developers: husky is not installed.",
    "test": "node --run lint && node --run test:spelling",
    "test:spelling": "cspell ."
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.3.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "cspell": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-import-x": "^4.16.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2"
  }
}
